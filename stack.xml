<stack>
  <description brief="rqt_pr2_dashboard">
      rqt_pr2_dashboard is a GUI for debugging and controlling low-level state of the PR2.  It shows things like battery status and breaker states, as well as integrating tools like rqt_console and robot_monitor.
  </description>
  <author>Aaron Blasdel</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rqt_pr2_dashboard</url>

  <depend stack="common_msgs"/> <!-- diagnostic_msgs -->
  <depend stack="pr2_common"/> <!-- pr2_msgs -->
  <depend stack="pr2_power_drivers"/> <!-- pr2_power_board -->
  <depend stack="ros"/> <!-- roslib -->
  <depend stack="ros_comm"/> <!-- rospy, std_srvs -->
  <depend stack="rqt"/> <!-- rqt_gui -->
  <depend stack="rqt_robot_plugins"/> <!-- rqt_robot_dashboard -->
  <depend stack="std_msgs"/> <!-- std_msgs -->
</stack>
